        <span class="nv">sb</span><span class="o">.</span><span class="py">append</span><span class="o">(</span><span class="s">"|"</span><span class="o">)</span>
        <span class="nv">sb</span><span class="o">.</span><span class="py">append</span><span class="o">(</span><span class="n">serverTimestamp</span><span class="o">)</span>
