      <span class="c1">//result.saveAsHadoopFile(HDFS_DIR, classOf[String], classOf[String], classOf[LauncherMultipleTextOutputFormat[String, String]])</span>
      <span class="nv">result</span><span class="o">.</span><span class="py">saveAsTextFile</span><span class="o">(</span><span class="nc">HDFS_DIR</span><span class="o">)</span>

    <span class="o">})</span>

    <span class="nv">ssc</span><span class="o">.</span><span class="py">start</span><span class="o">()</span>
    <span class="c1">// ç­‰å¾…å®æ—¶æµ</span>
    <span class="nv">ssc</span><span class="o">.</span><span class="py">awaitTermination</span><span class="o">()</span>
  <span class="o">}</span>

  <span class="c1">// åˆ†ææ—¥å¿—</span>
  <span class="k">def</span> <span class="nf">parseLog</span><span class="o">(</span><span class="n">log</span><span class="k">:</span> <span class="kt">String</span><span class="o">)</span><span class="k">:</span> <span class="o">(</span><span class="kt">String</span><span class="o">,</span> <span class="kt">String</span><span class="o">)</span> <span class="k">=</span> <span class="o">{</span>

    <span class="k">var</span> <span class="n">data</span> <span class="k">=</span> <span class="n">log</span>
    <span class="k">var</span> <span class="n">key</span> <span class="k">=</span> <span class="s">""</span>
    <span class="k">try</span> <span class="o">{</span>
      <span class="k">val</span> <span class="nv">sb</span> <span class="k">=</span> <span class="k">new</span> <span class="nc">StringBuilder</span>
      <span class="k">var</span> <span class="n">idx</span> <span class="k">=</span> <span class="mi">0</span>
      <span class="c1">// æ¥æ”¶æœåŠ¡å™¨è·å–çš„IP</span>
      <span class="k">val</span> <span class="nv">ip</span> <span class="k">=</span> <span class="nv">data</span><span class="o">.</span><span class="py">substring</span><span class="o">(</span><span class="mi">0</span><span class="o">,</span> <span class="nv">log</span><span class="o">.</span><span class="py">indexOf</span><span class="o">(</span><span class="s">"|"</span><span class="o">))</span>
      <span class="n">data</span> <span class="k">=</span> <span class="nv">data</span><span class="o">.</span><span class="py">substring</span><span class="o">(</span><span class="nv">log</span><span class="o">.</span><span class="py">indexOf</span><span class="o">(</span><span class="s">"|"</span><span class="o">)</span> <span class="o">+</span> <span class="mi">1</span><span class="o">)</span>
      <span class="nf">print</span><span class="o">(</span><span class="n">data</span><span class="o">)</span>

      <span class="k">var</span> <span class="n">tss</span> <span class="k">=</span> <span class="nv">data</span><span class="o">.</span><span class="py">substring</span><span class="o">(</span><span class="mi">0</span><span class="o">,</span> <span class="nv">data</span><span class="o">.</span><span class="py">indexOf</span><span class="o">(</span><span class="s">"|"</span><span class="o">))</span>
      <span class="n">tss</span> <span class="k">=</span> <span class="nv">tss</span><span class="o">.</span><span class="py">replaceAll</span><span class="o">(</span><span class="s">"\\."</span><span class="o">,</span> <span class="s">""</span><span class="o">)</span>
      <span class="c1">// æœåŠ¡å™¨æ—¶é—´æˆ³ï¼Œ13ä½</span>
      <span class="k">val</span> <span class="nv">serverTimestamp</span> <span class="k">=</span> <span class="nv">tss</span><span class="o">.</span><span class="py">toLong</span>
      <span class="k">val</span> <span class="nv">df</span> <span class="k">=</span> <span class="k">new</span> <span class="nc">SimpleDateFormat</span><span class="o">(</span><span class="s">"yyyy-MM-dd HH:mm:ss"</span><span class="o">)</span>
      <span class="c1">// æœåŠ¡å™¨æ—¶é—´</span>
      <span class="k">val</span> <span class="nv">serverTime</span> <span class="k">=</span> <span class="nv">df</span><span class="o">.</span><span class="py">format</span><span class="o">(</span><span class="n">serverTimestamp</span><span class="o">)</span>
      <span class="n">data</span> <span class="k">=</span> <span class="nv">data</span><span class="o">.</span><span class="py">substring</span><span class="o">(</span><span class="nv">data</span><span class="o">.</span><span class="py">indexOf</span><span class="o">(</span><span class="s">"|"</span><span class="o">)</span> <span class="o">+</span> <span class="mi">1</span><span class="o">)</span>
      <span class="k">val</span> <span class="nv">keyDf</span> <span class="k">=</span> <span class="k">new</span> <span class="nc">SimpleDateFormat</span><span class="o">(</span><span class="s">"yyyy/MM/dd/"</span><span class="o">)</span>
      <span class="n">key</span> <span class="k">=</span> <span class="nv">keyDf</span><span class="o">.</span><span class="py">format</span><span class="o">(</span><span class="n">serverTimestamp</span><span class="o">)</span>

      <span class="c1">// ä¸»ä½“æ•°æ®ï¼šimeiåˆ—è¡¨&amp;å“ç‰Œ&amp;æœºå‹|åŒ…å&amp;æ–‡ä»¶crc&amp;æ—¶é—´æˆ³åˆ—è¡¨;åŒ…å&amp;æ–‡ä»¶crc&amp;æ—¶é—´æˆ³åˆ—è¡¨;...</span>
      <span class="n">data</span> <span class="k">=</span> <span class="nv">data</span><span class="o">.</span><span class="py">replaceAll</span><span class="o">(</span><span class="s">"\\*"</span><span class="o">,</span> <span class="s">"\\/"</span><span class="o">)</span>
      <span class="n">data</span> <span class="k">=</span> <span class="nv">DataUtil</span><span class="o">.</span><span class="py">unzip</span><span class="o">(</span><span class="n">data</span><span class="o">)</span>

      <span class="c1">// è§£å‹åçš„æ•°æ®æ ¼å¼</span>
      <span class="c1">// 865066038753146-865066038753153&amp;meizu&amp;m5note|com.android.dialer&amp;1c5441a9&amp;1493109297305,1493110895807,1493112513536,1493112521588,1493112580015,1493112744680,1493138770296;cn.kuwo.player&amp;d1d03a24&amp;1493109460580;com.android.alarmclock&amp;3a6bc24a&amp;1493109495248,1493113574037,1493147103188;com.android.settings&amp;e49b42d4&amp;1493109801783,1493109835854,1493110147845,1493110230365,1493110320930,1493110344154,1493110350860,1493110796087,1493112348524,1493112850520,1493113933383,1493114212423,1493146022049,1493146963011;</span>
      <span class="k">val</span> <span class="nv">dataArr</span> <span class="k">=</span> <span class="nv">data</span><span class="o">.</span><span class="py">split</span><span class="o">(</span><span class="s">"\\|"</span><span class="o">)</span>
      <span class="k">val</span> <span class="nv">phoneData</span> <span class="k">=</span> <span class="nf">dataArr</span><span class="o">(</span><span class="mi">0</span><span class="o">).</span><span class="py">split</span><span class="o">(</span><span class="s">"&amp;"</span><span class="o">)</span>
      <span class="c1">// imeiåˆ—è¡¨</span>
      <span class="k">val</span> <span class="nv">imeis</span> <span class="k">=</span> <span class="nf">phoneData</span><span class="o">(</span><span class="mi">0</span><span class="o">).</span><span class="py">split</span><span class="o">(</span><span class="s">"-"</span><span class="o">).</span><span class="py">mkString</span><span class="o">(</span><span class="s">";"</span><span class="o">)</span>
      <span class="c1">// å“ç‰Œ</span>
      <span class="k">val</span> <span class="nv">phoneBrand</span> <span class="k">=</span> <span class="nf">phoneData</span><span class="o">(</span><span class="mi">1</span><span class="o">)</span>
      <span class="c1">// æœºå‹</span>
      <span class="k">val</span> <span class="nv">phoneModel</span> <span class="k">=</span> <span class="nf">phoneData</span><span class="o">(</span><span class="mi">2</span><span class="o">)</span>

      <span class="k">val</span> <span class="nv">appData</span> <span class="k">=</span> <span class="nf">dataArr</span><span class="o">(</span><span class="mi">1</span><span class="o">).</span><span class="py">split</span><span class="o">(</span><span class="s">";"</span><span class="o">)</span>
      <span class="k">val</span> <span class="nv">appArr</span> <span class="k">=</span> <span class="nc">ArrayBuffer</span><span class="o">[</span><span class="kt">String</span><span class="o">]()</span>
      <span class="nf">for</span> <span class="o">(</span><span class="n">app</span> <span class="k">&lt;-</span> <span class="n">appData</span><span class="o">)</span> <span class="o">{</span>
        <span class="k">val</span> <span class="nv">appDetail</span> <span class="k">=</span> <span class="nv">app</span><span class="o">.</span><span class="py">split</span><span class="o">(</span><span class="s">"&amp;"</span><span class="o">)</span>
        <span class="c1">// æ—¶é—´æˆ³åˆ—è¡¨</span>
        <span class="k">val</span> <span class="nv">timestamps</span> <span class="k">=</span> <span class="nf">appDetail</span><span class="o">(</span><span class="mi">2</span><span class="o">).</span><span class="py">split</span><span class="o">(</span><span class="s">","</span><span class="o">)</span>
