        <span class="nv">sb</span><span class="o">.</span><span class="py">append</span><span class="o">(</span><span class="nf">if</span> <span class="o">(</span><span class="nv">StringUtils</span><span class="o">.</span><span class="py">isNotBlank</span><span class="o">(</span><span class="n">serverTime</span><span class="o">))</span> <span class="n">serverTime</span> <span class="k">else</span> <span class="s">" "</span><span class="o">)</span>
